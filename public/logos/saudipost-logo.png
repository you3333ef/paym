    function serviceCatalogSearch() {
        var queryValue = $("#servicecatalogInput").val();
        if (queryValue.toLowerCase().includes("<script>") == true || queryValue.toLowerCase().includes("<") || queryValue.toLowerCase().includes(">")) {
            alert("Enter Valid Keyword")
            return;
        }
        window.location.href = "?query=" + queryValue;
    }

    function branchesearch() {
        const findBranchInput = $("#findBranchInput");
        const queryValue = findBranchInput.val().trim();
        if (queryValue) {
            findBranchInput.removeClass("border border-danger");
            window.location.href = `/${site.utils.language().url}/branches?query=${encodeURIComponent(queryValue)}`;
        }
        else {
            findBranchInput.addClass("border border-danger");
        }
    }
