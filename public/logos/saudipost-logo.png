<script>
    //var x = document.getElementById("demo");
    function getLocation() {
        if (navigator.geolocation) {
            navigator.geolocation.getCurrentPosition(showPosition);
        } else {
            alert( "Geolocation is not supported by this browser.")
        }
    }

    function showPosition(position) {
        var latitude = position.coords.latitude;
        var longitude = position.coords.longitude;
        $.ajax({
            url: "https://geolocation-db.com/jsonp",
            jsonpCallback: "callback",
            dataType: "jsonp",
            success: function (location) {
                var cityName = location.city;
                if (cityName.toLowerCase() == "riyadh" && 'ar' == "ar") {
                    cityName = "الرياض"
                }
                else if(cityName.toLowerCase() == "jeddah" && 'ar' == "ar") {
                    cityName = "جدة"
                };
                window.location.href = "branches?city=" + cityName + "&latitude=" + latitude + "&longitude=" + longitude;

            }
        });
        //console.log(latitude + " and " + longitude);
    }
</script>

<script type="text/javascript">
    $(document).ready(function () {
        //for grid view responsive
        var headertext = [],
            headers = document.querySelectorAll(".table th"),
            tablerows = document.querySelectorAll(".table th"),
            tablebody = document.querySelector(".table tbody");
        for (var i = 0; i < headers.length; i++) {

            var current = headers[i];
            headertext.push(current.textContent.replace(/\r?\n|\r/, ""));
        }
        if (tablebody !== null) {

            for (var i = 0, row; row = tablebody.rows[i]; i++) {
                for (var j = 0, col; col = row.cells[j]; j++) {
                    col.setAttribute("data-th", headertext[j]);
                }
            }
        }
        setTimeout(function () {
            $("#scrollToBranch").on("click", function () {
                document.querySelector('.find_branch').scrollIntoView({
                    behavior: 'smooth',
                    block: 'nearest',
                    inline: 'nearest'
                });
            });
        }, 100);
    });

    $(function () {
        var results = $(".branches_search_results");
        if (results.length) {
            var resultOffset = results.offset().top;
            $(window).scroll(function () {
                var windowOffset = $(window).scrollTop();
                var branchMap = $(".branch_locator_map");
                $(branchMap).toggleClass("fix-pos", windowOffset > resultOffset);
                if ($(window).scrollTop() + $(window).height() > $(document).height() - 200) {
                    $(".branch_locator_map").removeClass("fix-pos");
                }
            });
        }
        $(".embed-responsive-item").contents().find("body").css({ 'background': 'transparent' });
    });
</script>