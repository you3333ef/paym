    $(document).ready(function () {
        var cookieValue = sessionStorage['menu'];
        var menu = sessionStorage['menu'];
         var url = location.href.toLowerCase();//.indexOf("/government/") > 0
        if (menu == "enterprise" || url.indexOf("/enterprise") > 0) {
            $("#individual").removeClass('active');
            $("#government").removeClass('active');
            $("#enterprise").addClass('active');
            showMenu("enterprise");
        }
         else if (menu == "government" || url.indexOf("/government") > 0) {
            $("#individual").removeClass('active');
            $("#government").addClass('active');
            $("#enterprise").removeClass('active');
            showMenu("government");

        }
        else {
            $("#individual").addClass('active');
            $("#government").removeClass('active');
            $("#enterprise").removeClass('active');
            showMenu("individual");

        }
    })

    function showMenu(id) {
        if (id == "individual") {
            $('.menuIndividual').css("display", "flex");
            $('.menuGovernmental').css("display", "none");
            $('.menuEnterprise').css("display", "none");
            sessionStorage['menu'] = "individual";

            $(".sending_has_never").show();
            $("#nasherDiv").hide();

            $(".g20_section").show();
            $("#tjariDiv").hide();

        }
        else if (id == "government") {
            $('.menuGovernmental').css("display", "flex");
            $('.menuEnterprise').css("display", "none");
            $('.menuIndividual').css("display", "none");
            sessionStorage['menu'] = "government";
            $(".sending_has_never").hide();
            $("#nasherDiv").hide();
            $(".g20_section").hide();
            $("#tjariDiv").hide();

        }
        else {
            $('.menuEnterprise').css("display", "flex");
            $('.menuIndividual').css("display", "none");
            $('.menuGovernmental').css("display", "none");
            sessionStorage['menu'] = "enterprise";

            $(".sending_has_never").hide();
            $("#nasherDiv").show();

            $(".g20_section").hide();
            $("#tjariDiv").show();

        }
    }

